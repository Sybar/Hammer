<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="boss" maxOccurs="unbounded" minOccurs="1">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="game" maxOccurs="unbounded" minOccurs="3">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="page" maxOccurs="unbounded" minOccurs="1">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="options" maxOccurs="unbounded" minOccurs="1">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="stage" maxOccurs="unbounded" minOccurs="1">
																	<xs:complexType>
																		<xs:attribute type="xs:int" name="level" use="required" />
																		<xs:attribute type="xs:float" name="chance" use="required" />
																		<xs:attribute type="xs:int" name="primarySkillLevel" use="required" />
																		<xs:attribute type="xs:int" name="secondarySkillLevel" use="optional" />
																		<xs:attribute type="xs:int" name="combatPower" use="optional" />
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute type="xs:int" name="maxLevel" use="optional" />
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute type="xs:int" name="index" use="required" />
												<xs:attribute type="xs:float" name="successRate" use="required" />
												<xs:attribute type="xs:int" name="cardCount" use="optional" />
												<xs:attribute type="xs:int" name="primarySkillId" use="required" />
												<xs:attribute type="xs:int" name="secondarySkillId" use="optional" />
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attribute type="xs:string" name="type" use="required" />
									</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute type="xs:int" name="index" use="required" />
						<xs:attribute type="xs:int" name="specialSlotsCount" use="optional" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>