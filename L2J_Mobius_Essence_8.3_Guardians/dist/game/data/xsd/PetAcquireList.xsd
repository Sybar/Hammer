<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="list">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" name="pet">
					<xs:complexType>
						<xs:sequence>
							<xs:element maxOccurs="unbounded" name="skill">
								<xs:complexType>
									<xs:attribute name="id" type="xs:unsignedShort" use="required" />
									<xs:attribute name="lvl" type="xs:unsignedByte" use="required" />
									<xs:attribute name="reqLvl" type="xs:unsignedByte" use="required" />
									<xs:attribute name="evolve" type="xs:unsignedByte" use="required" />
									<xs:attribute name="item" type="xs:string" use="optional" />
									<xs:attribute name="itemAmount" type="xs:string" use="optional" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="type" type="xs:unsignedShort" use="required" />
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>